#include <stdio.h>

int main() {
   int array[10] = {16, 22, 38, 84, 13, 06, 57, 89, 125, 12};
   int loop, largest;
   int count=0;

   largest = array[0];
   
   for(loop = 1; loop < 10; loop++) {
   	count++;
      if( largest < array[loop] ) 
         largest = array[loop];
   }
   count++;
   count++;
   
   printf("Largest element of array is %d\n", largest);   
   printf("time complexity:%d",count);
   
   return 0;
}
